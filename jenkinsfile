pipeline {
    agent any
    
  environment {
     PASS=credentials('DOCKER_HUB')
  
  
  }

  stages {

      stage('Building jar'){
          steps {

              sh '''
                  ./jenkins-script/build/mvn.sh mvn -B -DskipTests clean package
                  ./jenkins-script/build/build.sh
              '''

          }
      }
  

      stage('Test'){
          steps{

              sh '''
                ./jenkins-script/test/test.sh mvn test

              '''
          }
      }
    
     stage('Push'){
         steps{
             
             sh '''
                ./jenkins-script/push/push.sh
                
             '''
            }
     }


  }  

  }


  
    



  









